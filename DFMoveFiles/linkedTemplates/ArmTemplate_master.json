{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DFMoveFiles"
		},
		"AzureBlobStorageLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLS'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"AzureSQLTest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLTest'"
		},
		"Dataverse1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Dataverse1'"
		},
		"DynamicsCrm1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'DynamicsCrm1'"
		},
		"OData1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OData1'"
		},
		"SynapseLink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SynapseLink'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synapsedesdl.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synapsedesdl.dfs.core.windows.net/"
		},
		"Dataverse1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "m.figini@consulcesi.com"
		},
		"DynamicsCrm1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "m.figini@consulcesi.com"
		},
		"OData1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://analytics.dev.azure.com/ConsulcesiDevTeams/Application/_odata/v3.0-preview/"
		},
		"OData1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "."
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DFMoveFiles_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageLS_connectionString": {
						"value": "[parameters('AzureBlobStorageLS_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"AzureSQLTest_connectionString": {
						"value": "[parameters('AzureSQLTest_connectionString')]"
					},
					"Dataverse1_password": {
						"value": "[parameters('Dataverse1_password')]"
					},
					"DynamicsCrm1_password": {
						"value": "[parameters('DynamicsCrm1_password')]"
					},
					"OData1_password": {
						"value": "[parameters('OData1_password')]"
					},
					"SynapseLink_connectionString": {
						"value": "[parameters('SynapseLink_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"Dataverse1_properties_typeProperties_username": {
						"value": "[parameters('Dataverse1_properties_typeProperties_username')]"
					},
					"DynamicsCrm1_properties_typeProperties_username": {
						"value": "[parameters('DynamicsCrm1_properties_typeProperties_username')]"
					},
					"OData1_properties_typeProperties_url": {
						"value": "[parameters('OData1_properties_typeProperties_url')]"
					},
					"OData1_properties_typeProperties_userName": {
						"value": "[parameters('OData1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DFMoveFiles_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DFMoveFiles_ArmTemplate_0')]"
			]
		}
	]
}